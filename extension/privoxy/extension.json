{
  "bin": "privoxy",
  "pre": "cat ./profile/{{Bypass}} >> {{ ExtensionDir }}/head.action; sed '1s/alias/{alias}/' {{ ExtensionDir }}/head.action > {{ ExtensionDir }}/user.action",
  "args": "--no-daemon {{ ExtensionDir }}/config",
  "exitargs": "",
  "keys": ["Protocol", "ServerAddress", "ServerPort", "Bypass"],
  "http": true,
  "socks5": false,
  "render": {"{{ ExtensionDir }}/config.jinja": "{{ ExtensionDir }}/config", "{{ ExtensionDir }}/actionhead.jinja":"{{ ExtensionDir }}/head.action"},
  "default": {
    "ServerAddress": "127.0.0.1"
  }
}