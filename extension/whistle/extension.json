{
  "bin": "w2",
  "args": "start -p {{ ExtensionPort }} -D '{{ ExtensionDir }}'",
  "url": "http://127.0.0.1:{{ ExtensionPort }}/#Network",
  "exitargs": "stop",
  "keys": ["ServerProtocol", "Rule"],
  "http": true,
  "socks5": true,
  "render": {"{% if Rule %}{{ ProfileDir }}/{{ Rule }}{% else %}{{ ExtensionDir }}/template.jinja{% endif %}": "{{ ExtensionDir }}/.whistle/rules/properties"},
  "default": {}
}